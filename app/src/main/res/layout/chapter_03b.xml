<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.naver.techcon.soup.Chapter03b">

    <ImageView
        android:id="@+id/thumbnail5"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="90dp"
        android:scaleType="centerCrop"
        android:src="@color/surface_02"
        app:layout_constraintBottom_toBottomOf="@id/thumbnail6"
        app:layout_constraintEnd_toEndOf="@id/thumbnail6" />

    <ImageView
        android:id="@+id/thumbnail6"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="120dp"
        android:background="@color/alert"
        android:scaleType="centerCrop"
        android:stateListAnimator="@animator/state_list_animator"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/thumbnail7"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="90dp"
        android:scaleType="centerCrop"
        android:src="@color/surface_02"
        app:layout_constraintStart_toStartOf="@id/thumbnail6"
        app:layout_constraintTop_toTopOf="@id/thumbnail6" />

</androidx.constraintlayout.widget.ConstraintLayout>
